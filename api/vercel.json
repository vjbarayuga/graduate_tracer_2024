{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        { "handle": "filesystem" },
        { "src": "/personal-info", "dest": "/index.js" },
        { "src": "/educational-attainment", "dest": "/index.js" },
        { "src": "/trainings", "dest": "/index.js" },
        { "src": "/employment", "dest": "/index.js" },
        { "src": "/user/:username", "dest": "/index.js" },
        { "src": "/search-users", "dest": "/index.js" },
        { "src": "/register", "dest": "/index.js" },
        { "src": "/login", "dest": "/index.js" },
        { "src": "/profile", "dest": "/index.js" },
        { "src": "/logout", "dest": "/index.js" }
    ]
}
