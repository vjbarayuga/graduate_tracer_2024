{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        { "handle": "filesystem" },
        { "src": "/api/personal-info", "dest": "/index.js" },
        { "src": "/api/educational-attainment", "dest": "/index.js" },
        { "src": "/api/trainings", "dest": "/index.js" },
        { "src": "/api/employment", "dest": "/index.js" },
        { "src": "/api/user/:username", "dest": "/index.js" },
        { "src": "/api/search-users", "dest": "/index.js" },
        { "src": "/api/register", "dest": "/index.js" },
        { "src": "/api/login", "dest": "/index.js" },
        { "src": "/api/profile", "dest": "/index.js" },
        { "src": "/api/logout", "dest": "/index.js" },
        { "src": "/(.*)", "dest": "/index.js" }
    ]
}
